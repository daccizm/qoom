<%- model_class = Room -%>
<div class="span3"></div>

<div class="span6">

  <div class="page-header">
    <h1>Roomを選択</h1>
  </div>
  <table class="table table-striped">
    <thead>
      <tr>
        <th><%=t '.title', :default => model_class.model_name.human %></th>
      </tr>
    </thead>
    <tbody>
      <% @rooms.each do |room| %>
        <tr>
          <td><%= link_to room.name, new_room_question_path( room.id ) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

</div>

<div class="span3"></div>
